@model zpnet.Models.Rental

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Rental</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Indeks" class="control-label"></label>
                <input asp-for="Indeks" class="form-control" />
                <span asp-validation-for="Indeks" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Produkcja" class="control-label"></label>
                <input asp-for="Produkcja" class="form-control" />
                <span asp-validation-for="Produkcja" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Cena" class="control-label"></label>
                <input asp-for="Cena" class="form-control" />
                <span asp-validation-for="Cena" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Data_r" class="control-label"></label>
                <input asp-for="Data_r" class="form-control" />
                <span asp-validation-for="Data_r" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Data_z" class="control-label"></label>
                <input asp-for="Data_z" class="form-control" />
                <span asp-validation-for="Data_z" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ClientId" class="control-label"></label>
                <select asp-for="ClientId" class="form-control" asp-items="ViewBag.ClientId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to Rental</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
